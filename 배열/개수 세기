#include <iostream>

int main()
{
    std::ios::sync_with_stdio(0);
	std::cin.tie(0);
	std::cout.tie(0);

	int n,v;
	std::cin >> n;

	int array[101];

	for (int i = 0; i < n; ++i)
	{
		std::cin >> array[i];
	}
	std::cin >> v;
	int count = 0;
	for (int i =0; i <n; ++i)
	{
		if (array[i] == v)
		{
			++count;
		}
	}
	std::cout << count;
    
}
