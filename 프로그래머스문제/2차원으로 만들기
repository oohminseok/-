#include <string>
#include <vector>

using namespace std;

vector<vector<int>> solution(vector<int> num_list, int n) 
{
    int count=0;
    vector<vector<int>> answer;
    vector<int> temp;
    for(int i=0; i<num_list.size(); ++i)
    {
        temp.push_back(num_list[i]);
        ++count;
        if (count == n)
		{
			answer.push_back(temp);
			temp.clear();
            count=0;
		}
        
    }
    
    
    return answer;
}
