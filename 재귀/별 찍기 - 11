#include<iostream>

const int MAX = 1024 * 3 + 2;

int n;
char board[MAX][MAX * 2 - 1];

void asterisk(const int& x, const int& y) 
{
    board[x][y] = '*';
    board[x + 1][y - 1] = '*', board[x + 1][y + 1] = '*';

    for (int i = y - 2; i <= y + 2; ++i) 
    {
        board[x + 2][i] = '*';
    }
}

void func(int n, int x, int y)
{
    if (n == 3) 
    {
        asterisk(x, y);
        return;
    }

    int ns = s / 2;
    func(ns, x, y);
    func(ns, x + ns, y - ns);
    func(ns, x + ns, y + ns);
}

int main() 
{
    std::cin >> n;
    func(n, 0, n - 1);

    for (int i = 0; i < n; ++i) 
    {
        for (int ii = 0; ii < n * 2 - 1; ++ii) 
        {
            if (board[i][ii] == '*')
            {
                std::cout << '*';
            }
            else
            {
                std::cout << ' ';
            }
        }
        std::cout << "\n";
    }
}
